<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-2.0.xsd">
 <changeSet id="1" author="sayooj">
 <createTable tableName="users">
 <column name="username" type="varchar(45)" >
 <constraints primaryKey="true" nullable="false"/>
 </column>

 <column name="password" type="varchar(45)" />
 <column name="firstname" type="varchar(45)" />
 <column name="lastname" type="varchar(45)" />
  <column name="email" type="varchar(45)" />
   <column name="address" type="varchar(45)" />
    <column name="phone" type="integer" />

 </createTable>
 
 </changeSet>
 </databaseChangeLog>